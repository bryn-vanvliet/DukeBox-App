{
  "compilerOptions": {
    "types": ["vite/client"],
    "target": "es2020", 
    "jsx": "react-jsx",
    "module": "ESNext",
    "moduleResolution": "Bundler", 
    "skipLibCheck": true,
    "paths": {
      "@/*": ["./src/*"]
  },
    "noEmit": true,
    "isolatedModules": true, 
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "allowImportingTsExtensions": true,
    "allowJs": true
}
}